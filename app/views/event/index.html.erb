<div class='text-center'>
  <h1>Events</h1>
  <% if organizer_permissions? %>
    <h3><%= link_to("Create New Event", new_event_path)%></h3>
  <% end %>
</div> <hr>
<% @events.each do |event| %>
  <h3><%= link_to(event.title, event_path(event)) %></h3>
  <p><%= event.description %></p>
  <hr>
<% end %>